  </animation>

  <model>
    <name>RightNavLight</name>
    <path>Aircraft/c182rg/Models/BeaconLight.xml</path>
    <offsets>
      <x-m>0.18</x-m>
      <y-m>5.27</y-m>
      <z-m>0.49</z-m>
    </offsets>
  </model>

  <animation>
    <type>material</type>
    <object-name>RightNavLight</object-name>
    <emission>
      <red>0.0</red>
      <green>1.0</green>
      <blue>0.0</blue>
      <factor-prop>/controls/switches/nav-lights</factor-prop>
    </emission>
  </animation>

  <model>
    <name>RearNavLight</name>
    <path>Aircraft/c182rg/Models/BeaconLight.xml</path>
    <offsets>
      <x-m>5.97</x-m>
