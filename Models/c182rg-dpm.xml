  <!-- Gear Animation - nosewheel steering-->
  <animation>
    <type>rotate</type>
    <object-name>NoseWheel</object-name>
    <object-name>NoseWheelStrut</object-name>
    <object-name>NoseWheelFork</object-name>
    <property>surface-positions/rudder-pos-norm</property>
    <factor>20</factor>
    <axis>
      <x1-m>-0.633</x1-m>
      <y1-m>0.0</y1-m>
      <z1-m>-0.706</z1-m>
      <x2-m>-0.773</x2-m>
      <y2-m>0.0</y2-m>
      <z2-m>-1.181</z2-m>
    </axis>
  </animation>
  
  <!-- Gear Animation - nosewheel -->
  <animation>
    <type>rotate</type>
    <object-name>NoseWheel</object-name>
    <object-name>NoseWheelStrut</object-name>
    <object-name>NoseWheelFork</object-name>
    <property>gear/gear[0]/position-norm</property>
    <interpolation>
      <entry>
        <ind>0</ind>
        <dep>95</dep>
      </entry>
      <entry>
        <ind>0.3</ind>
        <dep>95</dep>
      </entry>
      <entry>
        <ind>1.0</ind>
        <dep>0</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>-0.639</x-m>
      <y-m>0.0</y-m>
      <z-m>-0.722</z-m>
