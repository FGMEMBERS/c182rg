<!-- ALL-GLASS-GUAGES-AND-WINDSHIELD -->
<!--Working For ANY GLASS AND Rembrandt-->

<animation>
<object-name>vitre</object-name>
</animation>

<animation>
<type>noshadow</type>
<object-name>vitre</object-name>
</animation>

<animation>
<type>shader</type>
<shader>chrome</shader>
<texture>Aircraft/Gazelle/Models/Effects/gazelle-glass.png</texture>
<object-name>vitre</object-name>
</animation>

<!--Working For Rembrandt-->

  <!-- LOD for the model as a whole -->

  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-m>10000</max-m>
  </animation>

  <!-- LOD for specific objects -->

  <animation>
    <name>InteriorGroup</name>
    <object-name>lights</object-name>
    <object-name>propcontrolsurround</object-name>
    <object-name>PropControl</object-name>
    <object-name>mixturesurround</object-name>
    <object-name>Mixture</object-name>
    <object-name>Throttle</object-name>
    <object-name>throttlesurround</object-name>
    <object-name>Pedestal</object-name>
    <object-name>MagCompass</object-name>
    <object-name>FlapControl</object-name>
    <object-name>RightYoke</object-name>
    <object-name>LeftYoke</object-name>
    <object-name>TrimWheel</object-name>
    <object-name>Panel</object-name>
  </animation>

  <animation>
    <type>range</type>
    <object-name>InteriorGroup</object-name>
    <object-name>Pedal.1</object-name>
    <object-name>Pedal.2</object-name>
    <object-name>Pedal.3</object-name>
    <object-name>Pedal.4</object-name>
    <object-name>Pitot</object-name>
    <object-name>Ariel</object-name>
    <object-name>BeaconOff</object-name>
    <object-name>TailBeacon</object-name>
    <object-name>LeftFuelCap</object-name>
    <object-name>RightFuelCap</object-name>
    <min-m>0</min-m>
    <max-m>100</max-m>
  </animation>

  <animation>
    <type>material</type>
    <object-name>InteriorGroup</object-name>
    <emission>
      <red>0.45</red>
      <green>0.09</green>
      <blue>0.000045</blue>
      <factor-prop>/controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>

  <!-- Allow the yokes to be toggled on/off. -->
  <animation>
    <type>select</type>
    <object-name>LeftYoke</object-name>
    <object-name>RightYoke</object-name>
    <condition>
      <equals>
        <property>sim/model/hide-yoke</property>
        <value>false</value>
      </equals>
    </condition>
  </animation>

  <!-- Object movements -->
  <animation>
    <type>select</type>
    <object-name>Propeller</object-name>
    <condition>
      <less-than>
        <property>engines/engine[0]/rpm</property>
        <value>500</value>
      </less-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>Propeller.2</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value>400</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>blend</type>
    <object-name>Propeller.2</object-name>
    <property>engines/engine[0]/rpm</property>
    <factor>0.0004</factor>
    <max>1.0</max>
    <min>0.0</min>
  </animation>


  <animation>
    <type>translate</type>
    <object-name>Throttle</object-name>
    <property>controls/engines/engine[0]/throttle</property>
    <factor>-0.088</factor>
    <axis>
      <x>1.0</x>
      <y>0.0</y>
      <z>0.0</z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>Mixture</object-name>
    <property>controls/engines/engine[0]/mixture</property>
    <factor>-0.08</factor>
    <axis>
      <x>1.0</x>
      <y>0.0</y>
      <z>0.0</z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>PropControl</object-name>
    <property>controls/engines/engine[0]/propeller-pitch</property>
    <factor>-0.08</factor>
    <axis>
      <x>1.0</x>
      <y>0.0</y>
      <z>0.0</z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>Pedal.1</object-name>
    <object-name>Pedal.3</object-name>
    <property>controls/flight/rudder</property>
    <factor>0.1</factor>
    <axis>
      <x>1.0</x>
      <y>0.0</y>
      <z>0.0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Pedal.1</object-name>
    <object-name>Pedal.3</object-name>
    <property>controls/gear/brake-left</property>
    <factor>-15</factor>
    <axis>
      <x>0.0</x>
      <y>1.0</y>
      <z>0.0</z>
    </axis>
    <center>
      <x-m>-0.1</x-m>
      <y-m>0</y-m>
      <z-m>-0.9</z-m>
    </center>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>Pedal.2</object-name>
    <object-name>Pedal.4</object-name>
    <property>controls/flight/rudder</property>
    <factor>-0.1</factor>
    <axis>
      <x>1.0</x>
      <y>0.0</y>
      <z>0.0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Pedal.2</object-name>
    <object-name>Pedal.4</object-name>
    <property>controls/gear/brake-right</property>
    <factor>-15</factor>
    <axis>
      <x>0.0</x>
      <y>1.0</y>
      <z>0.0</z>
    </axis>
    <center>
      <x-m>-0.1</x-m>
      <y-m>0</y-m>
      <z-m>-0.9</z-m>
    </center>
  </animation>


  <animation>
    <type>spin</type>
    <object-name>Propeller</object-name>
    <property>engines/engine[0]/rpm</property>
    <factor>-1</factor>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>-0.214544</z-m>
    </center>
    <axis>
      <x>1.0</x>
      <y>0.0</y>
      <z>0.0</z>
    </axis>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>Propeller.2</object-name>
    <property>engines/engine[0]/rpm</property>
    <factor>-1</factor>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>-0.214544</z-m>
    </center>
    <axis>
      <x>1.0</x>
      <y>0.0</y>
      <z>0.0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>LeftAileron</object-name>
    <property>surface-positions/left-aileron-pos-norm</property>
    <interpolation>
      <entry>
        <ind>-1.0</ind>
        <dep>15</dep>
      </entry>
      <entry>
        <ind>0.0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>1.0</ind>
        <dep>-20</dep>
      </entry>
    </interpolation>
    <axis>
      <x1-m>1.22</x1-m>
      <y1-m>-2.546</y1-m>
      <z1-m>0.334</z1-m>
      <x2-m>0.761</x2-m>
      <y2-m>-5.101</y2-m>
      <z2-m>0.419</z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>RightAileron</object-name>
    <property>surface-positions/left-aileron-pos-norm</property>
    <interpolation>
      <entry>
        <ind>-1.0</ind>
        <dep>15</dep>
      </entry>
      <entry>
        <ind>0.0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>1.0</ind>
        <dep>-20</dep>
      </entry>
    </interpolation>
    <axis>
      <x1-m>1.22</x1-m>
      <y1-m>2.546</y1-m>
      <z1-m>0.334</z1-m>
      <x2-m>0.761</x2-m>
      <y2-m>5.101</y2-m>
      <z2-m>0.419</z2-m>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>LeftFlap</object-name>
    <property>surface-positions/flap-pos-norm</property>
    <factor>30</factor>
    <axis>
      <x1-m>1.219</x1-m>
      <y1-m>-2.542</y1-m>
      <z1-m>0.332</z1-m>
      <x2-m>1.131</x2-m>
      <y2-m>-0.599</y2-m>
      <z2-m>0.315</z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>RightFlap</object-name>
    <property>surface-positions/flap-pos-norm</property>
    <factor>30</factor>
    <axis>
      <x1-m>1.131</x1-m>
      <y1-m>0.599</y1-m>
      <z1-m>0.315</z1-m>
      <x2-m>1.219</x2-m>
      <y2-m>2.542</y2-m>
      <z2-m>0.332</z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>LeftElevator</object-name>
    <property>surface-positions/elevator-pos-norm</property>
    <interpolation>
      <entry>
        <ind>-1.0</ind>
        <dep>-28</dep>
      </entry>
      <entry>
        <ind>0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>1.0</ind>
        <dep>23</dep>
      </entry>
    </interpolation>
    <axis>
      <x1-m>5.138</x1-m>
      <y1-m>-1.657</y1-m>
      <z1-m>-0.475</z1-m>
      <x2-m>5.133</x2-m>
      <y2-m>-0.081</y2-m>
      <z2-m>-0.475</z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>RightElevator</object-name>
    <property>surface-positions/elevator-pos-norm</property>
    <interpolation>
      <entry>
        <ind>-1.0</ind>
        <dep>-28</dep>
      </entry>
      <entry>
        <ind>0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>1.0</ind>
        <dep>23</dep>
      </entry>
    </interpolation>
    <axis>
      <x1-m>5.133</x1-m>
      <y1-m>0.081</y1-m>
      <z1-m>-0.475</z1-m>
      <x2-m>5.138</x2-m>
      <y2-m>1.657</y2-m>
      <z2-m>-0.475</z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Rudder</object-name>
    <property>surface-positions/rudder-pos-norm</property>
    <factor>17.5</factor>
    <axis>
      <x1-m>4.976</x1-m>
      <y1-m>0.0</y1-m>
      <z1-m>-0.41</z1-m>
      <x2-m>5.938</x2-m>
      <y2-m>0</y2-m>
      <z2-m>0.942</z2-m>
    </axis>
  </animation>

  <!-- Chrome animation for spinner and exhaust-->
  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>chrome2.rgb</texture>
    <object-name>spinner</object-name>
    <object-name>exhaust</object-name>
  </animation>

  <!-- Lights -->

  <model>
    <name>LeftNavLight</name>
    <path>Aircraft/c182rg/Models/BeaconLight.xml</path>
    <offsets>
      <x-m>0.18</x-m>
      <y-m>-5.27</y-m>
      <z-m>0.49</z-m>
    </offsets>
  </model>

  <animation>
    <type>material</type>
    <object-name>LeftNavLight</object-name>
    <emission>
      <red>1.0</red>
      <green>0.0</green>
      <blue>0.0</blue>
