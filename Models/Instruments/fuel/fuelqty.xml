<!--Working For Rembrandt-->

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <emission>
 <red>1.0</red>
			<green>1.0</green>
			<blue>1.0</blue>
<factor-prop>/controls/lighting/dash-dim</factor-prop>
    </emission>
  </animation>

 
  <animation>
    <type>material</type>
    <object-name>fond</object-name>
  
    <emission>
        <red>1.0</red>
			<green>1.0</green>
			<blue>1.0</blue>
<factor-prop>/controls/lighting/landing-lights</factor-prop>
    </emission>
  </animation>








	
  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>/consumables/fuel/tank/level-lbs</property>
    <interpolation>
      <entry><ind>    0 </ind><dep>   0 </dep></entry>
      <entry><ind> 2000 </ind><dep> 240 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

